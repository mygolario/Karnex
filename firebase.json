{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "headers": [
      {
        "source": "**/*.png",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          }
        ]
      }
    ]
  }
}
